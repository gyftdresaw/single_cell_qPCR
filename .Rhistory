qPCR_load = function(data_file){
cell_ids = scan(data_file,nlines=1,what=character(),sep='\t')
cell_types = scan(data_file,skip=1,nlines=1,what=character(),sep='\t')
# now read data
qPCR.df = read.table(data_file,skip=2,sep='\t')
return(qPCR.df)
}
